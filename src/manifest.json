{
  "name": "kino",
  "version": "1.1.0",
  "description": "Гадание кинопоиска",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.kinopoisk.ru/*"],
      "js": ["pageHunter.js"]
    }
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
